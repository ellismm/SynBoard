<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="1%"
    android:verticalGap=".75%"
    android:background="@drawable/round_key"
    android:layout_gravity="center_horizontal"
    android:keyWidth="13%p"
    android:keyHeight="6%p">



    <Row android:keyHeight="8.5%p">

        <Key android:codes="38" android:keyLabel="&amp;" />
        <Key android:codes="45" android:keyLabel="-"/>
        <Key android:codes="49" android:keyIcon="@drawable/one" android:keyWidth="14.5%p"/>
        <Key android:codes="50" android:keyIcon="@drawable/two" android:keyWidth="14.5%p"/>
        <Key android:codes="51" android:keyIcon="@drawable/three" android:keyWidth="14.5%p"/>
        <Key android:codes="39" android:keyLabel="'"/>
        <Key android:codes="34" android:keyLabel='"' android:keyEdgeFlags="right"/>
    </Row>


    <Row android:keyHeight="8.5%p">
        <Key android:codes="58" android:keyLabel=":"/>
        <Key android:codes="59" android:keyLabel=";"/>
        <Key android:codes="52" android:keyIcon="@drawable/four"  android:keyWidth="14.5%p"/>
        <Key android:codes="53" android:keyIcon="@drawable/five"  android:keyWidth="14.5%p"/>
        <Key android:codes="54" android:keyIcon="@drawable/six"  android:keyWidth="14.5%p"/>
        <Key android:codes="43" android:keyLabel="+"/>
        <Key android:codes="64" android:keyLabel="\@" />

    </Row>



    <Row android:keyHeight="8.5%p">
        <Key android:codes="61" android:keyLabel="="/>
        <Key android:codes="47" android:keyLabel="&#47;"/>
        <Key android:codes="55" android:keyIcon="@drawable/seven"  android:keyWidth="14.5%p"/>
        <Key android:codes="56" android:keyIcon="@drawable/eight"  android:keyWidth="14.5%p"/>
        <Key android:codes="57" android:keyIcon="@drawable/nine"  android:keyWidth="14.5%p"/>
        <Key android:codes="63" android:keyLabel="\?"/>
        <Key android:codes="-5" android:keyIcon="@drawable/backspace"
            android:keyEdgeFlags="right"/>

    </Row>


    <Row>
        <Key android:codes="-2" android:keyLabel="abc"/>
        <Key android:codes="-3" android:keyLabel="Sym"/>
        <Key android:codes="32" android:keyLabel="space" android:keyWidth="29.5%p"/>
        <Key android:codes="48" android:keyIcon="@drawable/zero"  android:keyWidth="14.5%p"/>
        <Key android:codes="-4" android:keyIcon="@drawable/keyboard_return" android:keyWidth="27%p" android:keyEdgeFlags="right"/>
    </Row>

</Keyboard>



